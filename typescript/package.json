{
	"name": "coding-exercises",
	"version": "1.0.0",
	"license": "MIT",
	"scripts": {
		"all": "npm install && npm run build && npm test",
		"all:mealplan": "npm install && npm run build:mealplan && npm run test:mealplan",
		"all:helpsanta": "npm install && npm run build:helpsanta && npm run test:helpsanta",
		"all:maximizestocks": "npm install && npm run build:maximizestocks && npm run test:maximizestocks",
		"all:wrapgifts": "npm install && npm run build:wrapgifts && npm run test:wrapgifts",
		"all:seebuildingsleft": "npm install && npm run build:seebuildingsleft && npm run test:seebuildingsleft",
		"all:natoalphabet": "npm install && npm run build:natoalphabet && npm run test:natoalphabet",
		"all:longestsubsequence": "npm install && npm run build:longestsubsequence && npm run test:longestsubsequence",
		"all:anagrams": "npm install && npm run build:anagrams && npm run test:anagrams",
		"all:evaluate-postfix": "npm install && npm run build:evaluate-postfix && npm run test:evaluate-postfix",
		"all:available-numbers": "npm install && npm run build:available-numbers && npm run test:available-numbers",
		"build": "tsc --baseUrl ./src/**/solution/*",
		"build:mealplan": "tsc --baseUrl ./src/meal-planning/solution",
		"build:helpsanta": "tsc --baseUrl ./src/help-santa/solution",
		"build:maximizestocks": "tsc --baseUrl ./src/maximize-stocks/solution",
		"build:wrapgifts": "tsc --baseUrl ./src/wrap-gifts/solution",
		"build:seebuildingsleft": "tsc --baseUrl ./src/see-buildings-left/solution",
		"build:natoalphabet": "tsc --baseUrl ./src/nato-alphabet/solution",
		"build:longestsubsequence": "tsc --baseUrl ./src/longest-subsequence/solution",
		"build:anagrams": "tsc --baseUrl ./src/anagrams/solution",
		"build:evaluate-postfix": "tsc --baseUrl ./src/evaluate-postfix/solution",
		"build:available-numbers": "tsc --baseUrl ./src/available-numbers/solution",
		"test": "vitest run ./src/*/solution",
		"test:watch": "vitest ./src/*/solution",
		"test:mealplan": "vitest run ./src/meal-planning/solution",
		"test:watch:mealplan": "vitest ./src/meal-planning/solution",
		"test:helpsanta": "vitest run ./src/help-santa/solution",
		"test:watch:helpsanta": "vitest ./src/help-santa/solution",
		"test:maximizestocks": "vitest run ./src/maximize-stocks/solution",
		"test:watch:maximizestocks": "vitest ./src/maximize-stocks/solution",
		"test:wrapgifts": "vitest run ./src/wrap-gifts/solution",
		"test:watch:wrapgifts": "vitest ./src/wrap-gifts/solution",
		"test:seebuildingsleft": "vitest run ./src/see-buildings-left/solution",
		"test:watch:seebuildingsleft": "vitest ./src/see-buildings-left/solution",
		"test:natoalphabet": "vitest run ./src/nato-alphabet/solution",
		"test:watch:natoalphabet": "vitest ./src/nato-alphabet/solution",
		"test:longestsubsequence": "vitest run ./src/longest-subsequence/solution",
		"test:watch:longestsubsequence": "vitest ./src/longest-subsequence/solution",
		"test:anagrams": "vitest run ./src/anagrams/solution",
		"test:watch:anagrams": "vitest ./src/anagrams/solution",
		"test:evaluate-postfix": "vitest run ./src/evaluate-postfix/solution",
		"test:watch:evaluate-postfix": "vitest ./src/evaluate-postfix/solution",
		"test:available-numbers": "vitest run ./src/available-numbers/solution",
		"test:watch:available-numbers": "vitest ./src/available-numbers/solution",
		"test:coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "2.1.5",
		"typescript": "5.7.2",
		"vitest": "2.1.5"
	}
}
